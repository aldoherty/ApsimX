            <Code><![CDATA[using System;
using Models.Core;
using Models.PMF;
namespace Models
{
	[Serializable]
	public class Script : Model
	{
		[Link] Clock Clock;
		[Link] Fertiliser Fertiliser;
		[Link] Summary Summary;
		[Link] Plant Maize;
		
		
		[Description("Amount of fertiliser N to be applied (kg/ha)")]
		public double Amount { get; set; }
		
		[Description("Crop to be fertilised")]
		public string CropName { get; set; }
		
		[Description("Days after sowing:")]
		public int FertDAS { get; set; }
		
		

		[EventSubscribe("DoManagement")]
		private void OnDoManagement(object sender, EventArgs e)
		{
			if (Maize.IsAlive && Maize.Phenology.DaysAfterSowing == FertDAS)
				Fertiliser.Apply(Amount: Amount, Type: Fertiliser.Types.UreaN);
				
				
		}
		
	}
}
]]></Code>
